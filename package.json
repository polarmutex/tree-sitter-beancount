{
  "name": "tree-sitter-beancount",
  "version": "2.4.0",
  "description": "a tree-sitter parser for the beancount syntax",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bryall@github.com/bryall/tree-sitter-beancount.git"
  },
  "keywords": [
    "tree-sitter",
    "beancount"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Brian Ryall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bryall/tree-sitter-beancount/issues"
  },
  "homepage": "https://github.com/bryall/tree-sitter-beancount#readme",
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "0.25.4"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "0.25.4",
    "prebuildify": "^6.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "tree-sitter": [
    {
      "scope": "source.beancount",
      "injection-regex": "^beancount$"
    }
  ]
}
